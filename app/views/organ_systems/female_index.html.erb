<html>
  <head>
    <style type="text/css">
    body { background: #eef5fb !important; } /* Adding !important forces the browser to overwrite the default style applied by Bootstrap */
    .btn-primary {background: #2e353d !important; border: #2e353d !important ;}
    .btn {
      padding: 2.7% 0;
    }
    .responsive-width {
      font-size: 1.7vw;
    }
  </style>
  </head>

<body>
  <%= render "home/navbar" %>
  <!-- Page Content -->
    <div class="row systems">
      <div class="col-4 text-center" style="padding-top: 100px;">
        <%= render "fsidenav" %>
    </div>

    <div class="col-8 text-center">
          <br>
      <div class="container text-center" style="padding-top: 70px;">
        <h3 class="mt-5">FEMALE ORGAN SYSTEMS</h3>
        <p>Click on the name of an organ system to list its normal flora!</p>

        <iframe
  id="embedded-human"
  frameBorder="0"
  width="700"
  height="500"
  allowFullScreen="true"
  src="https://human.biodigital.com/widget/?be=2Wy5&background.colors=0,0,0,0,0,0,0,0&ui-fullscreen=false&ui-share=false&dk=e2c952a0399972a17607c3a82fb804513d551c19">
        </iframe>
    </div>
  </div>
</div>
<!-- Modal -->
<div class="modal fade michelle" id="exampleModalLong" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
      <div class="modal-header" style="background-color: #2e353d;">
        <%= image_tag "logo.png", :style => 'width: 250px; height: 50px;' %>

        <!-- <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5> -->
        <button type="button" class="close" data-dismiss="modal" aria-label="Close" style="color:white;">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">

      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-lg btn-primary" data-dismiss="modal">Close</button>

        <button type="button" class="btn btn-info btn-lg" data-toggle="modal" data-target="#test2">Open Modal 2
        </button>
      </div>
    </div>
  </div>
</div>

</body>

</html>
